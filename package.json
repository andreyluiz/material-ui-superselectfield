{
  "name": "material-ui-superselectfield",
  "version": "1.6.0",
  "description": "original Material-UI's SelectField component enhanced with autocompletion, multiselection, custom renderers, and infinite loading.",
  "private": false,
  "author": "RaphaÃ«l Morineau <raphael.morineau@gmail.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/Sharlaan/material-ui-superselectfield.git"
  },
  "homepage": "https://sharlaan.github.io/material-ui-superselectfield",
  "engines": {
    "node": ">=8"
  },
  "main": "lib/SuperSelectField.js",
  "dependencies": {
    "react-infinite": "^0.11.2"
  },
  "devDependencies": {
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-object-entries": "^1.0.0",
    "babel-plugin-transform-object-rest-spread": "^6.23.0",
    "babel-preset-env": "^1.6.0",
    "enzyme": "^2.9.1",
    "eslint": "^4.4.1",
    "flag-icon-css": "^2.8.0",
    "flow-bin": "^0.50.0",
    "gh-pages": "^1.0.0",
    "material-ui": "^0.18.7",
    "npm-run-all": "^4.0.2",
    "prop-types": "^15.5.10",
    "react": "^15.6.1",
    "react-dom": "^15.6.1",
    "react-router-dom": "^4.1.2",
    "react-scripts": "^1.0.11",
    "react-tap-event-plugin": "^2.0.1",
    "standard": "^10.0.3"
  },
  "peerDependencies": {
    "material-ui": ">= 0.15 < 1",
    "react": "^15.x",
    "react-dom": "^15.x",
    "react-tap-event-plugin": "^1.x || ^2.x"
  },
  "babel": {
    "plugins": [
      "transform-class-properties",
      "transform-object-rest-spread",
      "transform-object-entries"
    ],
    "presets": [
      [
        "env",
        {
          "targets": {
            "browsers": [
              "last 2 versions",
              "safari >= 7"
            ]
          }
        }
      ],
      "react"
    ]
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "build:dist": "babel src/SuperSelectField.js -o lib/SuperSelectField.js",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d build",
    "push": "git push",
    "prerelease": "run-s deploy build:dist push",
    "release": "npm publish",
    "flowversion": "flow version",
    "flowls": "cd src  &&  flow ls | xargs du -hs | sort -h",
    "typecheck": "cd src  &&  flow",
    "lint": "eslint src",
    "test": "react-scripts test --env=jsdom  --coverage",
    "eject": "react-scripts eject"
  }
}
